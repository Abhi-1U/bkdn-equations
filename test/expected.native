Pandoc
  Meta
    { unMeta =
        fromList
          [ ( "author" , MetaList [ MetaInlines [ Str "Abhi-1U" ] ] )
          , ( "title"
            , MetaInlines
                [ Str "Using"
                , Space
                , Str "bookdown"
                , Space
                , Str "equations"
                , Space
                , Str "Lua"
                , Space
                , Str "filter"
                ]
            )
          ]
    }
  [ Para
      [ Math
          DisplayMath "\\label{eq:emc.2}\n    e = m c^2 (\\#eq:emc-2)"
      , SoftBreak
      , Str "This"
      , Space
      , Str "is"
      , Space
      , Str "a"
      , Space
      , Str "new"
      , Space
      , Str "equation"
      , Space
      , RawInline (Format "markdown") "\\@ref(eq:emc-2)"
      ]
  ]
